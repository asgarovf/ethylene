{
  "name": "ethylene",
  "version": "1.2.5",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "Farhad Asgarov",
    "email": "fasgarov2002@gmail.com"
  },
  "description": "React hooks, utils, and UI components for interacting with Ethereum Smart Contracts",
  "repository": "https://github.com/asgarovf/ethylene",
  "dependencies": {
    "@reduxjs/toolkit": "1.8.1",
    "ethers": "5.6.6",
    "react-redux": "8.0.1"
  },
  "keywords": [
    "react",
    "ethers",
    "ethereum",
    "ethylene",
    "web3",
    "redux-toolkit"
  ],
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc --p tsconfig-build.json && cp package.json ./dist && cp README.md ./dist",
    "tsc": "npm run clean && tsc -p .",
    "share": "npm run build && npm publish ./dist",
    "share-prev": "npm run build && npm publish ./dist --tag prev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "16.11.33",
    "@types/react": "18.0.9"
  }
}
